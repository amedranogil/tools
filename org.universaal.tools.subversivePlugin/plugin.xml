<?xml version="1.0" encoding="UTF-8"?>
<?eclipse version="3.4"?>
<plugin>
   <extension
         point="org.eclipse.ui.commands">
      <category
            id="org.universaal.tools.subversivePlugin.commands.category"
            name="AAL Studio">
      </category>
      <command
            categoryId="org.universaal.tools.subversivePlugin.commands.category"
            id="org.universaal.tools.subversivePlugin.commands.exampleProject"
            name="Import example project">
      </command>
   </extension>
   <extension
         point="org.eclipse.ui.handlers">
      <handler
            class="org.universaal.tools.subversiveplugin.handlers.ImportExampleHandler"
            commandId="org.universaal.tools.subversivePlugin.commands.exampleProject">
      </handler>
   </extension>
   <extension
         point="org.eclipse.ui.menus">
      <menuContribution
            locationURI="menu:org.universaal.tools.AALStudio.menu?after=import">
         <command
               commandId="org.universaal.tools.subversivePlugin.commands.exampleProject"
               id="org.universaal.tools.subversivePlugin.commands.exampleProject">
         </command>
      </menuContribution>
   </extension>
   <extension
         point="org.eclipse.ui.preferencePages">
      <page
            category="org.universaal.tools.AALStudio"
            class="org.universaal.tools.subversiveplugin.preferences.PreferencePage"
            id="org.universaal.tools.subversiveplugin.preferences.PreferencePage"
            name="Import Example">
      </page>
   </extension>
   <extension
         point="org.eclipse.core.runtime.preferences">
      <initializer
            class="org.universaal.tools.subversiveplugin.preferences.PreferenceInitializer">
      </initializer>
   </extension>
   <extension
         point="org.eclipse.ui.importWizards">
      <wizard
            category="org.universaal.importexternalproject.import"
            class="org.universaal.tools.subversiveplugin.wizards.ImportExampleWizard"
            id="org.universaal.tools.subversivePlugin.wizard"
            name="Import Example">
      </wizard>
      <category
            id="org.universaal.importexternalproject.import"
            name="universAAL">
      </category>
   </extension>
   
</plugin>
