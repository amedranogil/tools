<?xml version="1.0" encoding="UTF-8"?>
<?eclipse version="3.4"?>
<plugin>

   <extension
         point="org.eclipse.ui.commands">
      <category
            name="Sample Category"
            id="org.universaal.tools.modelling.ontology.wizard.commands.category">
      </category>
      <command
            name="Sample Command"
            categoryId="org.universaal.tools.modelling.ontology.wizard.commands.category"
            id="org.universaal.tools.modelling.ontology.wizard.commands.sampleCommand">
      </command>
   </extension>
   <extension
         point="org.eclipse.ui.handlers">
      <handler
            commandId="org.universaal.tools.modelling.ontology.wizard.commands.sampleCommand"
            class="org.universaal.tools.modelling.ontology.wizard.handlers.NewOntologyProjectHandler">
      </handler>
   </extension>
   <extension
         point="org.eclipse.ui.bindings">
      <key
            commandId="org.universaal.tools.modelling.ontology.wizard.commands.sampleCommand"
            contextId="org.eclipse.ui.contexts.window"
            sequence="M1+6"
            schemeId="org.eclipse.ui.defaultAcceleratorConfiguration">
      </key>
   </extension>
   <extension
         point="org.eclipse.ui.menus">
      <menuContribution
            locationURI="menu:org.eclipse.ui.main.menu?after=additions">
         <menu
               label="Sample Menu"
               mnemonic="M"
               id="org.universaal.tools.modelling.ontology.wizard.menus.sampleMenu">
            <command
                  commandId="org.universaal.tools.modelling.ontology.wizard.commands.sampleCommand"
                  mnemonic="S"
                  id="org.universaal.tools.modelling.ontology.wizard.menus.sampleCommand">
            </command>
         </menu>
      </menuContribution>
      <menuContribution
            locationURI="toolbar:org.eclipse.ui.main.toolbar?after=additions">
         <toolbar
               id="org.universaal.tools.modelling.ontology.wizard.toolbars.sampleToolbar">
            <command
                  commandId="org.universaal.tools.modelling.ontology.wizard.commands.sampleCommand"
                  icon="icons/sample.gif"
                  tooltip="Say hello world"
                  id="org.universaal.tools.modelling.ontology.wizard.toolbars.sampleCommand">
            </command>
         </toolbar>
      </menuContribution>
   </extension>
   <extension
         point="org.eclipse.ui.newWizards">
      <category
            id="org.universaal.tools.newwizard.plugin"
            name="AAL Studio">
      </category>
      <wizard
            category="org.universaal.tools.newwizard.plugin"
            class="org.universaal.tools.modelling.ontology.wizard.wizards.OntologyProjectWizard"
            icon="icons/ic-uAAL-hdpi.png"
            id="org.universaal.tools.modelling.ontology.wizard.ontologyprojectwizard"
            name="New Ontology Project"
            project="true">
      </wizard>
   </extension>

</plugin>
