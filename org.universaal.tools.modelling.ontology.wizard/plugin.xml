<?xml version="1.0" encoding="UTF-8"?>
<?eclipse version="3.4"?>
<plugin>

   <extension
         point="org.eclipse.ui.commands">
      <category
            name="universAAL"
            id="org.universaal.tools.modelling.ontology.wizard.commands.category">
      </category>
      <command
            name="Create a new Ontology Project"
            categoryId="org.universaal.tools.modelling.ontology.wizard.commands.category"
            id="org.universaal.tools.modelling.ontology.wizard.commands.sampleCommand">
      </command>
   </extension>
   <extension
         point="org.eclipse.ui.handlers">
      <handler
            commandId="org.universaal.tools.modelling.ontology.wizard.commands.sampleCommand"
            class="org.universaal.tools.modelling.ontology.wizard.handlers.NewOntologyProjectHandler">
      </handler>
   </extension>
   <extension
         point="org.eclipse.ui.bindings">
      <key
            commandId="org.universaal.tools.modelling.ontology.wizard.commands.sampleCommand"
            contextId="org.eclipse.ui.contexts.window"
            sequence="M1+6"
            schemeId="org.eclipse.ui.defaultAcceleratorConfiguration">
      </key>
   </extension>
   <extension
         point="org.eclipse.ui.menus">
      <menuContribution
            locationURI="menu:org.universaal.tools.AALStudio.menu?after=new">
         <command
               commandId="org.universaal.tools.modelling.ontology.wizard.commands.sampleCommand"
               id="org.universaal.tools.modelling.ontology.wizard.menus.sampleCommand"
               mnemonic="S">
         </command>
      </menuContribution>
   </extension>
   <extension
         point="org.eclipse.ui.newWizards">
      <category
            id="org.universaal.tools.newwizard.plugin"
            name="AAL Studio">
      </category>
      <wizard
            category="org.universaal.tools.newwizard.plugin"
            class="org.universaal.tools.modelling.ontology.wizard.wizards.OntologyProjectWizard"
            icon="icons/ic-uAAL-hdpi.png"
            id="org.universaal.tools.modelling.ontology.wizard.ontologyprojectwizard"
            name="New Ontology Project"
            project="true">
      </wizard>
   </extension>

</plugin>
